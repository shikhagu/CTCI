// Partition linked list

1 -> 3 -> 2 -> 1 -> 8 -> 5 -> 5

threshold = 5
gePtr -> 8
lPtr -> null

public void partitionList(Node head, int threshold)
{
  Node gePtr = head;
  Node lPtr;
  
  while(gePtr != null)
  {
    if(gePtr.data < threshold)
      gePtr = gePtr.next;
    else
      break;
  }
  
  if(gePtr != null)
    lPtr = gePtr.next;
  
  while(lPtr != null)
  {
    if(lPtr.data < threshold)
    {
      int temp = gePtr.data;
      gePtr.data = lPtr.data;
      lPtr.data = temp;
      gePtr = gePtr.next;
    } 
    lPtr = lPtr.next;
  }
  
}
