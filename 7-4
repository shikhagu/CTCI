// ParkingLot
public class ParkingLot{
  private Level[] levels;
  public Spot assignSpot(Size size, Vehicle vehicle, String specialCondition); // Calls level and finds a spot atleast of that size
  public boolean performCostTransaction(String creditcard, String credentials, Spot spot); 
  public boolean isAvailable(Spot s);
}
public class ParkingCost{
  private double smallSpotCostPerHour;
  private double mediumSpotCostPerHour;
  private double largeSpotCostPerHour;
  public double computeCost(Size spotSize, DateTime startTime);
}
public class Level{
  private ArrayList<Spot> smallSpots;
  private ArrayList<Spot> mediumSpots;
  private ArrayList<Spot> largeSpots;
  private boolean isFull;
  public boolean assignSpot(Size size, Vehicle vehicle); // Assigns a spot to the vehicle
}
public class Spot{
  private Size size;
  private int locationId;
  private Vehicle currentVehicle;
  private DateTime startTime;
}
public enum Size{
  SMALL, MEDIUM, LARGE;
}
public class Vehicle{
  private String vehicleNumber;
}
